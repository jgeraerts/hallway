	<queryString>
		<![CDATA[select ROOMS.ROOMNUMBER,
          R.SURNAMEMOTHER || ' ' || R.GIVENNAMEMOTHER AS NAME,
          R.SURNAMEBABY   || ' ' || R.GIVENNAMEBABY   AS BABYNAME,
          R.GYNEACOLOGIST AS DOCTOR,
          R.NUTRITION AS NUTRITION,
          R.PEDIATRICIAN AS PEDIATRICIAN,
          DECODE(R.CAESARIAN, 1, 'S', 0, 'D') || ' ' || DATEDIFF('DAY',R.BIRTHDATE,CURRENT_DATE)  AS DAY,
         C.COMMENT AS COMMENT
from records r
right outer join rooms on r.ROOMNUMBER = rooms.ROOMNUMBER
left join comments c on c.recordid=r.id
left join (select recordid,max(commentdate) as commentdate from comments group by recordid) gc on ( gc.recordid = r.id and gc.commentdate = c.commentdate)]]>
	</queryString>
